@font-face {
  font-family: 'iconfont';
  src: url('data:font/ttf;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}


.iconfont {
  font-family: "iconfont" !important;
  font-size: 20px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconfont-wolf:before {
  content: "\e91f";
}

.iconfont-waves:before {
  content: "\ecd1";
}
.iconfont-dg-wolf:before {
  content: "\e609";
}